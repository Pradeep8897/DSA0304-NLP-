import spacy
try:
    nlp=spacy.load("en_core_web_sm")
except:
    import spacy.cli
    spacy.cli.download("en_core_web_sm")
    nlp=spacy.load("en_core_web_sm")
text="Apple is looking at buying U.K. startup for $1 billion."
doc=nlp(text)
for ent in doc.ents:
    print(ent.text, ent.label_)


output:
Apple ORG
U.K. GPE
$1 billion MONEY
